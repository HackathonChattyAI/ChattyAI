    "lint-staged": "13.2.3",
    "prettier": "3.0.0"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": "npm run lint",
    "*.{js, jsx,ts,tsx,md,html,json}": "npm run format"
