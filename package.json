{
  "name": "my-app",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=8.5"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.17.0",
