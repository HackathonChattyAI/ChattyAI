    "@testing-library/user-event": "13.5.0",
    "@types/dom-speech-recognition": "^0.0.1",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.38",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "dotenv": "16.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
